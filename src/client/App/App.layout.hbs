---
layout: main.layout
titleSeparator: '-'
titleName: 'ModularAdmin | HTML version'
---
<div
  id="App"
  class="
    App
    -header-fixed
    -header-fixed-mobile
    -sidebar-compact-tablet
    -sidebar-open-tablet
    -sidebar-compact-desktop
    -customize-open
  " >

  <div class="HeaderContainer">
    {{> App/_common/Header/Header}}
  </div>
  <div class="SidebarContainer">
    {{> App/_common/Sidebar/Sidebar}}
  </div>
  <div class="SidebarOverlay" id="SidebarOverlay"></div>
  <div class="ContentContainer {{pagename}}">
    <div class="ContentBackground"></div>
    <div class="Content">
      {{{body}}}
    </div>
  </div>

  <div class="FooterContainer" >
    {{> App/_common/Footer/Footer}}
  </div>

  {{> App/_common/Search/Search}}

  <div class="CustomizeContainer">
    {{> App/_common/Customize/Customize}}
  </div>

</div>
